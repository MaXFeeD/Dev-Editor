{
	"info": {
		"name": "Dev Editor",
		"version": "0.4-snapshot1",
		"author": "Nernar",
		"description": "Develop your mods with in-game tools.",
		"clientOnly": true,
		"instantLaunch": true
	},
	"api": "AdaptedScript",
	"resources": [],
	"sources": [
		{
			"source": "lib/*",
			"type": "library",
			"language": "javascript"
		},
		{
			"type": "launcher",
			"language": "javascript",
			"source": "script/launcher.js"
		},
		{
			"type": "main",
			"language": "javascript",
			"source": "script/main",
			"target": "script/main.js"
		},
		{
			"type": "custom",
			"language": "javascript",
			"source": "script/runtime",
			"target": "script/runtime.js"
		},
		{
			"type": "custom",
			"language": "javascript",
			"api": "Instant",
			"source": "script/instant.js"
		},
		{
			"type": "custom",
			"language": "javascript",
			"source": "script/adaptedscript",
			"target": "script/adaptedscript.js"
		},
		{
			"type": "custom",
			"language": "javascript",
			"source": "script/coreengine",
			"target": "script/coreengine.js"
		}
	],
	"compile": [
		{
			"source": "java/*",
			"type": "java"
		}
	],
	"additional": [
		{
			"source": "assets/root/*",
			"targetDir": "."
		}
	]
}
